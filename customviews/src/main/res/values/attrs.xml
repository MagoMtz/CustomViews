<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="isMandatory" format="boolean" />
    <attr name="title" format="string" />
    <attr name="onlyNumbers" format="boolean" />
    <attr name="charsWithBlankSpaces" format="boolean" />
    <attr name="allChars" format="boolean" />
    <attr name="onlyUpperCase" format="boolean" />
    <attr name="spinnerHeight" format="dimension" />
    <attr name="futureDate" format="boolean" />

    <declare-styleable name="CustomEditText">
        <attr name="onlyNumbers" />
        <attr name="charsWithBlankSpaces" />
        <attr name="allChars" />
        <attr name="onlyUpperCase" />
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleEditText">
        <attr name="onlyNumbers" />
        <attr name="charsWithBlankSpaces" />
        <attr name="allChars" />
        <attr name="onlyUpperCase" />
        <attr name="isMandatory" />
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="TextArea">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleTextArea">
        <attr name="isMandatory" />
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="CustomSpinner">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleSpinner">
        <attr name="title"/>
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="SearchableSpinner">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleSearchableSpinner">
        <attr name="title" />
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="MultiSelectSearchSpinner">
        <attr name="isMandatory" />
        <attr name="minSelection" format="integer" />
        <attr name="maxSelection" format="integer" />
    </declare-styleable>

    <declare-styleable name="SearchSpinner">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleSearchSpinner">
        <attr name="title" />
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleMultiSelectSearchSpinner">
        <attr name="title" />
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="SearchListSpinner">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleSearchListSpinner">
        <attr name="title" />
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="SingleTime">
        <attr name="isMandatory" />
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="TimeRange">
        <attr name="isMandatory" />
        <attr name="futureDate" />
        <attr name="initTimeTitle" format="string" />
        <attr name="finalTimeTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="SingleDate">
        <attr name="isMandatory" />
        <attr name="title" />
        <attr name="futureDate" />
    </declare-styleable>

    <declare-styleable name="DateRange">
        <attr name="isMandatory" />
        <attr name="futureDate" />
        <attr name="initDateTitle" format="string" />
        <attr name="finalDateTitle" format="string" />
        <attr name="daysRange" format="integer" />
    </declare-styleable>

    <declare-styleable name="EmailEditText">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleEmailEditText">
        <attr name="isMandatory" />
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="PhoneEditText">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitlePhoneEditText">
        <attr name="isMandatory" />
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="ZipCodeEditText">
        <attr name="isMandatory" />
    </declare-styleable>

    <declare-styleable name="TitleZipCodeEditText">
        <attr name="isMandatory" />
        <attr name="title" />
    </declare-styleable>

</resources>